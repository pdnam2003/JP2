<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demosql.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <TextField promptText="FirstName" fx:id="first" />
    <TextField promptText="LastName" fx:id="last" />
    <TextField promptText="Address" fx:id="address" />
    <Button text="Save" onAction="#onHelloButtonClick"/>


    <TableView fx:id="InforTable" layoutX="20" layoutY="250" prefWidth="600" prefHeight="300">
        <columns>
            <TableColumn fx:id="firstCol" text="First" prefWidth="120" />
            <TableColumn fx:id="lastCol" text="Last" prefWidth="120" />
            <TableColumn fx:id="addCol" text="Add" prefWidth="120" />
        </columns>
    </TableView>

</VBox>
